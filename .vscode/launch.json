    "version": "0.2.0",
    "configurations": [
    
        {
            "type": "antlr-debug",
            "request": "launch",
            "name": "Launch Program",
            "input": "input.txt",
            "grammar": "Programa.g4",
            "printParseTree": true,
            "visualParseTree": true,
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}"
        }
    ]
}
